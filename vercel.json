{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./src ./public ./index.html ./package.json ./vite.config.ts",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://your-backend-url.onrender.com/api/:path*"
    },
    {
      "source": "/ws/:path*",
      "destination": "https://your-backend-url.onrender.com/ws/:path*"
    }
  ]
}
