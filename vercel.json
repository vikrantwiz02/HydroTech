{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://your-backend-url.onrender.com/api/:path*"
    },
    {
      "source": "/ws/:path*",
      "destination": "https://your-backend-url.onrender.com/ws/:path*"
    }
  ]
}
